{
  "name": "lipe/chatapp",
  "description": "A chat application backend.",
  "require": {
    "php": "^8.2",
    "psr/log": "^1.1",
    "guzzlehttp/guzzle": "^7.0",
    "elasticsearch/elasticsearch": "^8.15",
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.6",
    "league/oauth2-server": "^9.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
